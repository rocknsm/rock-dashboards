{
  "order": 90,
  "index_patterns": [
    "ecs-suricata-*"
  ],
  "version": 2019081301,
  "settings": {
  },
  "mappings": {
    "properties": {
      "fileinfo": {
        "properties": {
          "filename": {
            "type": "alias",
            "path": "file.name"
          },
          "md5": {
            "type": "alias",
            "path": "file.hash.md5"
          },
          "sha1": {
            "type": "alias",
            "path": "file.hash.sha1"
          },
          "sha256": {
            "type": "alias",
            "path": "file.hash.sha256"
          },
          "sha512": {
            "type": "alias",
            "path": "file.hash.sha512"
          },
          "size": {
            "type": "alias",
            "path": "file.size"
          }
        }
      },
     "file": {
        "properties": {
          "accessed": {
            "type": "date"
          },
          "changed": {
            "type": "alias",
            "path": "file.ctime"
          },
          "created": {
            "type": "date"
          },
          "ctime": {
            "type": "date"
          },
          "device": {
            "copy_to": "related.domain",
            "ignore_above": 2048,
            "fields": {
              "text": {
                "type": "text"
              }
            },
            "type": "keyword"
          },
          "directory": {
            "ignore_above": 4096,
            "fields": {
              "text": {
                "type": "text"
              }
            },
            "type": "keyword"
          },
          "extension": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "gid": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "group": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "hash": {
            "properties": {
              "imphash": {
                "copy_to": "related.hash",
                "ignore_above": 4096,
                "type": "keyword"
              },
              "md5": {
                "copy_to": "related.hash",
                "ignore_above": 1024,
                "type": "keyword"
              },
              "murmur3": {
                "copy_to": "related.hash",
                "ignore_above": 1024,
                "type": "keyword"
              },
              "sha1": {
                "copy_to": "related.hash",
                "ignore_above": 1024,
                "type": "keyword"
              },
              "sha256": {
                "copy_to": "related.hash",
                "ignore_above": 1024,
                "type": "keyword"
              },
              "sha512": {
                "copy_to": "related.hash",
                "ignore_above": 1024,
                "type": "keyword"
              },
              "ssdeep": {
                "copy_to": "related.hash",
                "ignore_above": 4096,
                "type": "keyword"
              }
            }
          },
          "inode": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "mode": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "mtime": {
            "type": "date"
          },
          "modified": {
            "type": "alias",
            "path": "file.mtime"
          },
          "mime_type": {
            "ignore_above": 8000,
            "fields": {
              "text": {
                "type": "text"
              }
            },
            "type": "keyword"
          },
          "name": {
            "ignore_above": 8000,
